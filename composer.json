{
    "name": "ngabor84/guzzle-wsse-middleware",
    "description": "WSSE authentication middleware for Guzzle HTTP Client",
    "keywords": ["guzzle", "http", "middleware", "wsse"],
    "homepage": "https://github.com/ngabor84/guzzle-wsse-middleware",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabor Nemeth",
            "email": "negabor@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "^1.0",
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "check": "./vendor/bin/phpcs --colors",
        "fix": "./vendor/bin/phpcbf",
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "autoload": {
        "psr-4": {
            "Guzzle\\Http\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guzzle\\Http\\Middleware\\Tests\\": "test"
        }
    }
}
